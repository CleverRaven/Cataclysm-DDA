{
    "problemMatcher": [
        {
            "owner": "cata-clang",
            "pattern": [
                {
                    "regexp": "^(?:[0-9.:]+|\\(continued from above\\)) (ERROR) : (?:(?:\\.\\./)*)([^:]*):(\\d+) (.+)(?<!\\(json-error\\))$",
                    "severity": 1,
                    "file": 2,
                    "line": 3,
                    "message": 4
                }
            ]
        }
    ]
}
