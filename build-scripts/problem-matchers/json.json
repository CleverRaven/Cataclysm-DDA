{
    "problemMatcher": [
        {
            "owner": "cata-json",
            "pattern": [
                {
                    "regexp": "^Json error: ([^:]*):([0-9]+):([0-9]+): (.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        }
    ]
}
