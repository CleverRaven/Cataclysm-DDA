    "effect": [
      {
        "if": { "and": [ { "math": [ "LS_Passcode_Selector", "==", "3" ] }, { "u_has_trait": "LS_Passcode_3" } ] },
        "then": { "math": [ "LS_Passcode_Selector", "-=", "1" ] }
