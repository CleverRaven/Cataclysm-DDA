            { "u_at_om_location": "sylph_genius_loci_SW" },
            { "u_at_om_location": "sylph_genius_loci_SE" }
          ]
        }
      ]
    },
    "deactivate_condition": { "not": { "u_has_trait": "SYLPH" } },
    "effect": [ { "run_eocs": "EOC_SYLPH_SPELL_EXPERIENCE_INCREASER_SELECTOR" } ]
