[
  {
    "type": "effect_on_condition",
    "id": "EOC_LILIN_RUACH_DRAIN",
    "recurrence": [ "6 minutes", "6 minutes" ],
    "condition": { "and": [ { "u_has_trait": "LILIN_TRAITS" }, { "math": [ "u_vitamin('lilin_ruach_vitamin')", ">=", "1" ] } ] },
