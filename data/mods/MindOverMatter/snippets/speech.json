  {
    "type": "speech",
    "speaker": [ "mon_feral_security_captain_psychic", "mon_feral_security_psychic" ],
    "sound": "\"Stop!\"",
    "volume": 15
  },
  {
    "type": "speech",
    "speaker": [ "mon_feral_security_captain_psychic", "mon_feral_security_psychic" ],
    "sound": "\"This is a restricted area!\"",
