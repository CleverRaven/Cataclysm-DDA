    }
  },
  {
    "type": "effect_on_condition",
    "id": "EOC_TELEKINETIC_FORCE_SHOVE_SELECTOR_PUSH",
    "effect": { "u_cast_spell": { "id": "telekinetic_force_shove_away_target" }, "targeted": true }
  },
  {
