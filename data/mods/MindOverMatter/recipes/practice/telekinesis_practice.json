          },
          { "math": [ "u_val('vitamin', 'name:vitamin_psionic_drain')", "+=", "rng( 0,2 )" ] },
          { "run_eocs": "EOC_PSI_PRACTICE_FOCUS_MOD" }
        ],
        "false_effect": {
          "run_eocs": [
            {
