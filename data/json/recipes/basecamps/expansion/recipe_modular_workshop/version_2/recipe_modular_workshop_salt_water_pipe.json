    "//": "The blueprint is a template that will be rotated and mirrored to match its placement. Note that this does NOT",
    "//0": "have anything to do with the flags provided: they serve a different purpose. Also note that the name is hard",
    "//1": "coded to be picked up by code specific to this functionality. Also note that the non orthogonal orientations",
    "//2": "use the blueprint faction_expansion_salt_water_pipe_swamp_NE instead.",
    "blueprint_name": "salt water pipe construction",
    "blueprint_requires": [ { "id": "fbmw_2" } ],
    "blueprint_provides": [ { "id": "fbmw_2_salt_water_pipe_swamp" } ],
    "blueprint_excludes": [ { "id": "fbmw_2_salt_water_pipe_swamp" } ],
    "//3": "1 salt water well, 4 pits, and filling up 3 pits (the last one is for the well). 6 h, 40 m * 3, 30 m * 4, 150 m * 4",
    "blueprint_needs": {
      "time": "20 h",
      "skills": [ [ "fabrication", 4 ], [ "survival", 4 ] ],
      "inline": {
        "qualities": [ { "id": "HAMMER", "level": 2 }, { "id": "DIG", "level": 2 } ],
        "components": [ [ [ "rock", 40 ] ], [ [ "2x4", 4 ] ], [ [ "nail", 8 ] ], [ [ "pipe", 6 ] ], [ [ "pipe_fittings", 6 ] ] ]
      }
    },
    "check_blueprint_needs": false,
    "//4": "Abuse of the flags. Since the referenced blueprint isn't actually used, they don't have any actual effect",
