[
  {
    "type": "palette",
    "id": "construction_palette",
    "//": "intended for in-construction homes",
    "parameters": {
      "construction_exterior_wall_type": {
        "type": "ter_str_id",
        "default": {
          "distribution": [
            [ "t_brick_wall", 6 ],
            [ "t_rock_wall", 3 ],
            [ "t_wall_wood", 3 ],
            [ "t_concrete_wall", 4 ],
            [ "t_adobe_brick_wall", 1 ]
          ]
        }
      },
      "construction_interior_wall_type": {
        "type": "ter_str_id",
        "default": {
          "distribution": [
            [ "t_wall_half", 26 ],
            [ "t_wall_wood", 13 ],
            [ "t_wall_b", 1 ],
            [ "t_wall_g", 1 ],
            [ "t_wall_p", 1 ],
            [ "t_wall_P", 1 ],
            [ "t_wall_r", 1 ],
            [ "t_wall_w", 2 ],
            [ "t_wall_y", 1 ],
            [ "t_wall_gray", 1 ],
            [ "t_wall_brown", 1 ],
            [ "t_wall_cyan", 1 ],
            [ "t_wall_black", 1 ],
            [ "t_wall_orange", 1 ]
          ]
        }
      },
      "construction_sidewalk_type": { "type": "ter_str_id", "default": { "distribution": [ [ "t_sidewalk", 3 ], [ "t_region_soil", 2 ] ] } },
      "construction_house_concrete_outside_type": {
        "type": "ter_str_id",
        "default": { "distribution": [ [ "t_thconc_floor", 3 ], [ "t_pit_shallow", 2 ], [ "t_region_soil", 2 ] ] }
      },
      "construction_house_fence_type": {
        "type": "ter_str_id",
        "default": {
          "distribution": [
            [ "t_splitrail_fence", 3 ],
            [ "t_chainfence", 2 ],
            [ "t_fence", 2 ],
            [ "t_privacy_fence", 1 ],
            [ "t_drystone_wall_half", 1 ],
            [ "t_fence_post", 8 ],
            [ "t_chainfence_posts", 4 ],
            [ "t_region_soil", 8 ]
          ]
        }
      },
      "construction_house_doors_outside_type": {
        "type": "ter_str_id",
        "default": {
          "distribution": [
            [ "t_floor", 4 ],
            [ "t_door_frame", 4 ],
            [ "t_door_locked_peep", 1 ],
            [ "t_door_locked", 2 ],
            [ "t_door_elocked_peep", 1 ],
            [ "t_door_elocked", 2 ],
            [ "t_door_c", 2 ],
            [ "t_door_o", 1 ]
          ]
        }
      }
    },
    "toilets": { "t": {  } },
    "terrain": {
      "c": { "param": "construction_house_concrete_outside_type", "fallback": "t_region_soil" },
      "f": { "param": "construction_house_fence_type", "fallback": "t_region_soil" },
      "g": {
        "switch": { "param": "construction_house_fence_type", "fallback": "t_region_soil" },
        "cases": {
          "t_splitrail_fence": "t_splitrail_fencegate_c",
          "t_chainfence": "t_chaingate_c",
          "t_fence": "t_fencegate_c",
          "t_privacy_fence": "t_privacy_fencegate_c",
          "t_drystone_wall_half": "t_chaingate_l",
          "t_fence_post": "t_region_soil",
          "t_chainfence_posts": "t_region_soil",
          "t_region_soil": "t_region_soil"
        }
      },
      "h": "t_pit_shallow",
      "o": [ [ "t_window", 2 ], [ "t_window_empty", 5 ] ],
      "p": {
        "switch": { "param": "construction_house_concrete_outside_type", "fallback": "t_region_soil" },
        "cases": { "t_thconc_floor": "t_pavement", "t_pit_shallow": "t_pit_shallow", "t_region_soil": "t_region_soil" }
      },
      ".": [ [ "t_region_groundcover_urban", 5 ], [ "t_region_soil", 10 ] ],
      "+": {
        "switch": { "param": "construction_house_doors_outside_type", "fallback": "t_door_frame" },
        "cases": {
          "t_floor": "t_floor",
          "t_door_frame": "t_door_frame",
          "t_door_c": "t_door_c",
          "t_door_o": "t_door_o",
          "t_door_locked_peep": "t_door_o",
          "t_door_locked": "t_door_c",
          "t_door_elocked_peep": "t_door_o",
          "t_door_elocked": "t_door_c"
        }
      },
      "*": { "param": "construction_house_doors_outside_type", "fallback": "t_door_frame" },
      "^": "t_gutter_downspout",
      "#": { "param": "construction_exterior_wall_type", "fallback": "t_brick_wall" },
      "|": { "param": "construction_interior_wall_type", "fallback": "t_wall_half" },
      ":": "t_wall_glass",
      "-": "t_sidewalk",
      "~": "t_linoleum_white",
      "<": "t_stairs_up",
      ">": "t_stairs_down",
      "`": { "param": "construction_sidewalk_type", "fallback": "t_region_soil" }
    },
    "furniture": {
      "C": [
        [ "f_null", 20 ],
        [ "f_pallet_brick", 2 ],
        [ "f_pallet_brick_adobe", 2 ],
        [ "f_pallet_cement", 2 ],
        [ "f_stack_plank", 3 ]
      ],
      "M": "f_mailbox",
      "S": "f_sink",
      "F": "f_fridge",
      "O": [ "f_oven", "f_gas_oven_microwave_combo" ],
      "7": "f_counter",
      "8": "f_cupboard",
      "9": "f_shower"
    },
    "nested": {
      "#": { "chunks": [ [ "shelter_graffiti", 1 ], [ "general_graffiti", 1 ], [ "null", 298 ] ] },
      "X": { "chunks": [ [ "null", 6 ], [ "construction_crate_open", 5 ], [ "construction_crate_closed", 1 ] ] },
      "~": { "chunks": [ [ "null", 9 ], [ "construction_bathroom_plumbing", 3 ], [ "construction_bathroom_tools", 1 ] ] },
      " ": {
        "chunks": [
          [ "corpse_blood_1x1", 1 ],
          [ "blood_field", 2 ],
          [ "bile_field", 1 ],
          [ "construction_supplies", 4 ],
          [ "null", 192 ]
        ]
      }
    }
  }
]
