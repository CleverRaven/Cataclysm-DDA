[
  {
    "type": "palette",
    "id": "construction_palette",
    "//": "intended for in-construction homes",
    "parameters": {
      "construction_exterior_wall_type": {
        "type": "ter_str_id",
        "default": {
          "distribution": [
            [ "t_brick_wall", 6 ],
            [ "t_rock_wall", 3 ],
            [ "t_wall_wood", 3 ],
            [ "t_concrete_wall", 4 ],
            [ "t_adobe_brick_wall", 1 ]
          ]
        }
      },
      "construction_sidewalk_type": { "type": "ter_str_id", "default": { "distribution": [ [ "t_sidewalk", 3 ], [ "t_region_soil", 2 ] ] } },
      "construction_house_concrete_outside_type": {
        "type": "ter_str_id",
        "default": { "distribution": [ [ "t_thconc_floor", 3 ], [ "t_pit_shallow", 2 ], [ "t_region_soil", 2 ] ] }
      },
      "construction_house_fence_type": {
        "type": "ter_str_id",
        "default": {
          "distribution": [
            [ "t_splitrail_fence", 3 ],
            [ "t_chainfence", 2 ],
            [ "t_fence", 2 ],
            [ "t_privacy_fence", 1 ],
            [ "t_drystone_wall_half", 1 ],
            [ "t_fence_post", 8 ],
            [ "t_chainfence_posts", 4 ],
            [ "t_region_soil", 8 ]
          ]
        }
      }
    },
    "toilets": { "t": {  } },
    "terrain": {
      "c": { "param": "construction_house_concrete_outside_type", "fallback": "t_region_soil" },
      "f": { "param": "construction_house_fence_type", "fallback": "t_region_soil" },
      "g": {
        "switch": { "param": "construction_house_fence_type", "fallback": "t_region_soil" },
        "cases": {
          "t_splitrail_fence": "t_splitrail_fencegate_c",
          "t_chainfence": "t_chaingate_c",
          "t_fence": "t_fencegate_c",
          "t_privacy_fence": "t_privacy_fencegate_c",
          "t_drystone_wall_half": "t_chaingate_l",
          "t_fence_post": "t_region_soil",
          "t_chainfence_posts": "t_region_soil",
          "t_region_soil": "t_region_soil"
        }
      },
      "h": "t_pit_shallow",
      "o": [ [ "t_window", 2 ], [ "t_window_empty", 5 ] ],
      "p": "t_pavement",
      ".": [ [ "t_region_groundcover_urban", 5 ], [ "t_region_soil", 10 ] ],
      "+": [ [ "t_door_b", 2 ], [ "t_door_frame", 2 ], [ "t_door_c", 5 ], [ "t_door_o", 5 ], [ "t_door_locked_interior", 1 ] ],
      "*": [
        [ "t_door_b", 5 ],
        [ "t_door_locked_peep", 2 ],
        "t_door_locked_alarm",
        [ "t_door_locked", 10 ],
        [ "t_door_elocked_peep", 4 ],
        "t_door_elocked_alarm",
        [ "t_door_elocked", 15 ],
        "t_door_c",
        [ "t_door_o", 2 ]
      ],
      "^": "t_gutter_downspout",
      "|": "t_wall_wood",
      "#": { "param": "construction_exterior_wall_type", "fallback": "t_brick_wall" },
      "$": "t_wall_half",
      ":": "t_wall_glass",
      "-": "t_sidewalk",
      "~": "t_linoleum_white",
      "<": "t_stairs_up",
      ">": "t_stairs_down",
      "`": { "param": "construction_sidewalk_type", "fallback": "t_region_soil" }
    },
    "furniture": {
      "C": [
        [ "f_null", 20 ],
        [ "f_pallet_brick", 2 ],
        [ "f_pallet_brick_adobe", 2 ],
        [ "f_pallet_cement", 2 ],
        [ "f_stack_plank", 3 ]
      ],
      "M": "f_mailbox",
      "S": "f_sink",
      "F": "f_fridge",
      "X": [ "f_crate_c", [ "f_crate_o", 5 ] ],
      "O": [ "f_oven", "f_gas_oven_microwave_combo" ],
      "7": "f_counter",
      "8": "f_cupboard",
      "9": "f_shower"
    },
    "items": {
      "X": [
        { "item": "hardware_bulk", "chance": 40, "repeat": [ 1, 2 ] },
        { "item": "wood_workshop", "chance": 40, "repeat": [ 1, 3 ] },
        { "item": "plumbing_bulk", "chance": 30, "repeat": [ 1, 2 ] }
      ],
      "~": [
        { "item": "floor_trash", "chance": 20 },
        { "item": "hardware_trash", "chance": 20 },
        { "item": "tools_plumbing", "chance": 5 },
        { "item": "supplies_plumbing", "chance": 10 }
      ]
    },
    "nested": {
      "#": { "chunks": [ [ "shelter_graffiti", 1 ], [ "general_graffiti", 1 ], [ "null", 298 ] ] },
      " ": {
        "chunks": [
          [ "corpse_blood_1x1", 1 ],
          [ "blood_field", 2 ],
          [ "bile_field", 1 ],
          [ "construction_supplies", 4 ],
          [ "null", 192 ]
        ]
      }
    }
  }
]
