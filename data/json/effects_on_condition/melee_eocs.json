    "global": false,
    "eoc_type": "EVENT",
    "required_event": "character_melee_attacks_monster",
    "condition": { "math": [ "_hits", "==", "1" ] },
    "effect": [ { "run_eocs": "EOC_MELEE_PROF_WHAT_CATEGORY" } ]
