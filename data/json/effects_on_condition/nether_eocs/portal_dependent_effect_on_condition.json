    "recurrence": [ "5 minutes", "15 minutes" ],
    "condition": {
      "and": [
        { "u_has_trait": "PORTAL_DEPENDENT" },
        {
          "or": [
            { "is_weather": "portal_storm" },
            { "and": [ { "is_weather": "early_portal_storm" }, { "x_in_y_chance": { "x": 5, "y": 10 } } ] },
            { "and": [ { "is_weather": "distant_portal_storm" }, { "x_in_y_chance": { "x": 6, "y": 10 } } ] },
