        "max_bonus": -21,
        "duration": "1 days",
        "decay_start": "1 days"
      }
    ],
    "false_effect": [ { "u_lose_morale": "morale_social_dissatisfied" } ]
  },
  {
    "type": "effect_on_condition",
    "id": "eoc_asocial_satisfied",
    "recurrence": [ "1 minutes", "5 minutes" ],
