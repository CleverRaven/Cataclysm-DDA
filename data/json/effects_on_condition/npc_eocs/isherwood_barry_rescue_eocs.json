            {
              "mapgen_update": "nest_Isherwood_firing_line_swat_add",
              "om_terrain": "rural_road",
              "om_special": "Barry's Mi-Go Scout Tower"
            }
          ]
        }
      },
      {
        "if": { "math": [ "isherwood_wall_status", "==", "1" ] },
