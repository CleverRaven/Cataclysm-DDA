          { "math": [ "currently_invading_map", "=", "1" ] }
        ],
        "else": {
          "if": { "math": [ "isherwood_family_coming", "!=", "1" ] },
          "then": [
            {
              "u_location_variable": { "global_val": "invasion_map" },
              "target_params": { "om_terrain": "barry_mi-go_scout_tower_1", "search_range": 500, "z": 0 }
