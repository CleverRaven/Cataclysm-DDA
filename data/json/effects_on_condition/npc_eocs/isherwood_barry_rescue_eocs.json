  {
    "type": "effect_on_condition",
    "id": "isherwood_mission_barry_return_home",
    "global": true,
    "eoc_type": "EVENT",
    "required_event": "avatar_enters_omt",
    "condition": {
      "and": [
        { "u_has_mission": "MISSION_ISHERWOOD_CHRIS_1" },
