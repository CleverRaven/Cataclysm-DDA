msgid "Exhausted"
msgstr "精疲力尽"

#: src/npctalk.cpp src/player.cpp
msgid "Dead tired"
msgstr "快累死了"

#: src/npctalk.cpp src/player.cpp
msgid "Tired"
msgstr "疲倦"

#: src/npctalk.cpp
msgid "Not tired"
msgstr "精神饱满"

#: src/npctalk.cpp
msgid "No, just <swear> no..."
msgstr "不行就是<swear> 不行。"

#: src/npctalk.cpp
msgid "Just let me sleep, <name_b>!"
msgstr "求你了让我睡觉吧， <name_b>！"

#: src/npctalk.cpp
msgid "Make it quick, I want to go back to sleep."
msgstr "快一点，我想回去睡觉"

