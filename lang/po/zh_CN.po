msgstr "巧克力包裹着麦芽糖，味道好极了"

#: lang/json/COMESTIBLE_from_json.py
msgid "blackberries"
msgstr "黑莓"

#: lang/json/COMESTIBLE_from_json.py
msgid "A darker cousin of raspberry."
msgstr "一种黑色的树莓"

#: lang/json/COMESTIBLE_from_json.py
msgid "cooked fruit"
msgstr "煮熟的水果"

