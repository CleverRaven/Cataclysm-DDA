msgid_plural "albino penguins"
msgstr[0] ""

#: lang/json/MONSTER_from_json.py
msgid "amigara horror"
msgid_plural "amigara horrors"
msgstr[0] "阿彌殼怪"

#: lang/json/MONSTER_from_json.py
msgid "amoebic mold"
msgid_plural "amoebic molds"
msgstr[0] "阿米巴黏菌"

#: lang/json/MONSTER_from_json.py
msgid "animatronic bat"
msgid_plural "animatronic bats"
msgstr[0] "蝙蝠機器人"

#: lang/json/MONSTER_from_json.py
msgid "animatronic beaver"
msgid_plural "animatronic beavers"
msgstr[0] "海貍機器人"

#: lang/json/MONSTER_from_json.py
msgid "animatronic cat"
msgid_plural "animatronic cats"
msgstr[0] "機器貓"

#: lang/json/MONSTER_from_json.py
msgid "animatronic child"
msgid_plural "animatronic childs"
msgstr[0] "孩童型機械布偶"

#: lang/json/MONSTER_from_json.py
