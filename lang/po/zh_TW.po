#: lang/json/json_materials.py:101 lang/json/json_materials.py:108
msgid "gouged"
msgstr "挖出的"

#: lang/json/json_materials.py:106
msgid "Plastic"
msgstr "塑膠的"

#: lang/json/json_materials.py:113
msgid "Glass"
msgstr "玻璃的"

#: lang/json/json_materials.py:120
msgid "Iron"
