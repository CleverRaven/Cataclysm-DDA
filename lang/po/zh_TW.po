#: lang/extra/android/options.cpp src/options.cpp
msgid "Switch between kg and lbs."
msgstr "於 公斤(kg) 與 磅(lbs) 之間切換。"

#: lang/extra/android/options.cpp src/options.cpp
msgid "Cup"
