msgstr "Zwrócę to z powrotem!"

#: src/npctalk.cpp
msgid "Give it to me, or else!"
msgstr "Daj mi to, albo...!"

#: src/npctalk.cpp
msgid "Eh, never mind."
msgstr "Eh, nieważne."

#: src/npctalk.cpp
msgid "Never mind, I'll do without.  Bye."
msgstr "Nieważne, obejdę się. Żegnaj."

#: src/npctalk.cpp
