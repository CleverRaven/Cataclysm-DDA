#: lang/extra/android/options.cpp src/options.cpp
msgid "options"
msgstr "opcje"

#: lang/json/AMMO_from_json.py
msgid "battery"
msgstr "bateria"

#. ~ Description for battery
#: lang/json/AMMO_from_json.py
msgid ""
"A set of universal batteries.  Used to charge almost any electronic device."
msgstr "Zestaw uniwersalnych baterii. Używane w większości urządzeń elektronicznych."

#: lang/json/AMMO_from_json.py
msgid "aluminum foil"
msgstr "folia aluminiowa"

#. ~ Description for aluminum foil
#: lang/json/AMMO_from_json.py
msgid ""
"A thin foil made of aluminum.  Sometimes called 'tin foil' due to being made"
" of tin in the past."
msgstr "Cienka folia wykonana z aluminium. Czasami nazywana 'folią cynową', ponieważ w przeszłości była robiona z cyny."

#: lang/json/AMMO_from_json.py
msgid "cent"
msgstr "cent"

#. ~ Description for cent
#. ~ Description for placeholder ammunition
#. ~ Description for software
#: lang/json/AMMO_from_json.py lang/json/GENERIC_from_json.py
msgid "IF YOU ARE SEEING THIS IT IS A BUG."
msgstr "JEŻELI TO WIDZISZ TO NAPOTKAŁEŚ BŁĄD."

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "thread"
msgstr "nić"

#. ~ Description for thread
#: lang/json/AMMO_from_json.py
msgid "A small quantity of thread that could be used to refill a sewing kit."
msgstr "Nieduża ilość nici, której można użyć do uzupełnienia zestawu do szycia."

#: lang/json/AMMO_from_json.py
msgid "sinew"
msgstr "ścięgna"

#. ~ Description for sinew
#: lang/json/AMMO_from_json.py
msgid "A tough sinew cut from a corpse, usable as thread."
msgstr "Wytrzymałe ścięgna wycięte z ciała, mogą zastąpić nici."

#: lang/json/AMMO_from_json.py
msgid "plant fiber"
msgstr "włókno roślinne"

#. ~ Description for plant fiber
#: lang/json/AMMO_from_json.py
msgid "Tough thin fiber, taken from a plant.  Can be used as thread."
msgstr "Wytrzymałe cienkie włókna pochodzenia roślinnego. Można ich używać jako nici."

#: lang/json/AMMO_from_json.py
msgid "yarn"
msgstr "przędza"

#. ~ Description for yarn
#: lang/json/AMMO_from_json.py
msgid "Wool yarn, could be used to knit wool clothing."
msgstr "Wełniana przędza, nadająca się do tkania i szydełkowania wełnianych ubrań."

#: lang/json/AMMO_from_json.py
msgid "soap bar"
msgstr "kostka mydła"

#. ~ Description for soap bar
#: lang/json/AMMO_from_json.py
msgid "A cleansing agent made into bars."
msgstr "Środek do czyszczenia w formie kostki."

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "duct tape"
msgstr "taśma klejąca"

#. ~ Description for duct tape
#: lang/json/AMMO_from_json.py
msgid "A roll of incredibly strong tape.  Its uses are innumerable."
msgstr "Rolka niezwykle wytrzymałej przemysłowej klejącej taśmy montażowej. Jej zastosowania są nieprzeliczone."

#: lang/json/AMMO_from_json.py
msgid "rolling paper"
msgstr "bibułka"

#. ~ Description for rolling paper
#: lang/json/AMMO_from_json.py
msgid "These are thin paper strips intended for the rolling of cigarettes."
msgstr "Te cienkie kawałki papieru służą do zwijania papierosów."

#: lang/json/AMMO_from_json.py
msgid "copper wire"
msgstr "miedziany przewód"

#. ~ Description for copper wire
#: lang/json/AMMO_from_json.py
msgid "Plastic jacketed copper cable of the type used in small electronics."
msgstr "Obleczone w plastik miedziane kable w rodzaju używanym w drobnej elektronice."

#: lang/json/AMMO_from_json.py
msgid "plutonium cell"
msgstr "ogniwo plutonowe"

#. ~ Description for plutonium cell
#: lang/json/AMMO_from_json.py
msgid ""
"A nuclear-powered battery.  Used to charge advanced and rare electronics."
msgstr "Ogniwo oparte na energii nuklearnej. Służy do zasilania zaawansowanej i rzadkiej egzotycznej elektroniki."

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "plutonium slurry"
msgstr "zawiesina plutonu"

#. ~ Description for plutonium slurry
#. ~ Description for watery plutonium slurry
#: lang/json/AMMO_from_json.py
msgid ""
"Highly radioactive slurry.  It is probably best to get far away from this."
msgstr "Wysoce radioaktywna zawiesina. Najlepiej byłoby zabrać się jak najdalej od niej."

#: lang/json/AMMO_from_json.py
msgid "watery plutonium slurry"
msgstr "rozwodniona zawiesina plutonu"

#: lang/json/AMMO_from_json.py
msgid "pebble"
msgstr "kamyk"

#. ~ Description for pebble
#: lang/json/AMMO_from_json.py
msgid "A handful of pebbles, useful as ammunition for slings or slingshots."
msgstr "Garść kamyków. Przydatne jako amunicja do procy."

#: lang/json/AMMO_from_json.py
msgid "marble"
msgstr "szklana kulka"

#. ~ Description for marble
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of glass marbles, useful as ammunition for slings or slingshots."
msgstr "Garść szklanych kulek. Posłużyć mogą jako amunicja do procy."

#: lang/json/AMMO_from_json.py
msgid "bearings"
msgstr "łożyska"

#. ~ Description for bearings
#: lang/json/AMMO_from_json.py
msgid "A box of ball bearings, useful as ammunition for slings."
msgstr "Paczka kulek do łożysk kulkowych, służących też za amunicję do procy."

#: lang/json/AMMO_from_json.py
msgid "BB"
msgstr "śrut"

#. ~ Description for BB
#: lang/json/AMMO_from_json.py
msgid "A box of small steel balls.  They deal virtually no damage."
msgstr "Pudełko śrutu do wiatrówki. Zadaje bardzo niewielkie obrażenia."

#: lang/json/AMMO_from_json.py
msgid "feather"
msgstr "pióro"

#. ~ Description for feather
#: lang/json/AMMO_from_json.py
msgid "Feathers from a bird.  Useful for fletching arrows"
msgstr "Ptasie pióra. Przydatne do tworzenia strzał."

#: lang/json/AMMO_from_json.py
msgid "down feather"
msgstr "pióro puchowe"

#. ~ Description for down feather
#: lang/json/AMMO_from_json.py
msgid "fluffy down feathers from a bird.  Useful for making cozy bedclothes."
msgstr "Puchate pierze wydarte z ptasich podbrzuszy. Użyteczne przy wykonywaniu miękkich pierzyn i poduszek."

#: lang/json/AMMO_from_json.py
msgid "fusion pack"
msgstr "pakiet fuzyjny"

#. ~ Description for fusion pack
#: lang/json/AMMO_from_json.py
msgid ""
"In the middle of the 21st Century, military powers began to look towards "
"energy based weapons.  The result was the standard fusion pack, capable of "
"delivering bolts of superheated gas at near light speed with no recoil."
msgstr "W połowie 21-go wieku siły wojskowe zwróciły się ku broni energetycznej. W efekcie powstały standaryzowane pakiety fuzyjne, zdolne bezodrzutowo miotać ładunkami ekstremalnie przegrzanego gazu osiągającego niemal prędkość światła."

#: lang/json/AMMO_from_json.py
msgid "66mm HEAT"
msgstr "66mm HEAT"

#. ~ Description for 66mm HEAT
#: lang/json/AMMO_from_json.py
msgid ""
"A 60mm High Explosive Anti Tank round.  It could blow through up to two feet"
" of concrete."
msgstr "60mm pocisk przeciwczołgowy z zintegorwanym ładunkiem wybuchowym. Przebija do dwóch stóp betonu."

#: lang/json/AMMO_from_json.py
msgid "120mm HEAT"
msgstr "120mm HEAT"

#. ~ Description for 120mm HEAT
#: lang/json/AMMO_from_json.py
msgid ""
"A 120mm High Explosive Anti Tank round.  It could ruin anyone's whole day."
msgstr "120mm pocisk przeciwczołgowy z zintegrowanym ładunkiem wybuchowym. Przebija do dwóch stóp betonu."

#: lang/json/AMMO_from_json.py
msgid "hydrogen canister"
msgstr "kanister z wodorem"

#. ~ Description for hydrogen canister
#: lang/json/AMMO_from_json.py
msgid ""
"A canister of pressurized hydrogen so heavily compressed it has turned "
"solid.  With proper equipment, it could be heated to plasma."
msgstr "Kanister wodoru pod ciśnieniem, który został tak mocno sprężony, że zamienił się w ciało stałe. Z odpowiednim wyposażeniem mógłby być zmieniony w plazmę."

#: lang/json/AMMO_from_json.py
msgid "unused .22 casing"
msgstr "nieużywane łuski .22"

#. ~ Description for unused .22 casing
#: lang/json/AMMO_from_json.py
msgid ""
"An empty casing from a .22 round that hasn't been fired, and still has an "
"intact case with primer."
msgstr "Pusta łuska z naboju .22, fabrycznie nowa, z nienaruszoną komorą spłonki."

#: lang/json/AMMO_from_json.py
msgid "gunpowder"
msgstr "proch strzelniczy"

#. ~ Description for gunpowder
#: lang/json/AMMO_from_json.py
msgid "Firearm quality gunpowder."
msgstr "Proch strzelniczy wysokiej jakości wykorzystywany w przemyśle zbrojeniowym."

#: lang/json/AMMO_from_json.py
msgid "oxidizer powder"
msgstr "proszek utleniający"

#. ~ Description for oxidizer powder
#: lang/json/AMMO_from_json.py
msgid "Volatile powdered chemical oxidizer."
msgstr "Nietrwała sproszkowana substancja chemiczna o silnych właściwościach utleniających."

#: lang/json/AMMO_from_json.py
msgid "lye powder"
msgstr "ług"

#. ~ Description for lye powder
#: lang/json/AMMO_from_json.py
msgid "Powdered caustic soda."
msgstr "Sproszkowany wodorotlenek sodu, znany jako ług lub soda kaustyczna. Wysoce zasadowy środek chemiczny o korodujących właściwościach."

#: lang/json/AMMO_from_json.py
msgid "shotgun primer"
msgstr "spłonka do strzelb"

#. ~ Description for shotgun primer
#: lang/json/AMMO_from_json.py
msgid "Primer from a shotgun round."
msgstr "Spłonka używana w nabojach do strzelb do inicjowania zapłonu ładunku właściwego."

#: lang/json/AMMO_from_json.py
msgid "small pistol primer"
msgstr "spłonka do niskokalibrowych pistoletów"

#. ~ Description for small pistol primer
#: lang/json/AMMO_from_json.py
msgid "Primer from a small caliber pistol round."
msgstr "Spłonka używana w nabojach pistoletowych niskiego kalibru do inicjowania zapłonu ładunku właściwego."

#: lang/json/AMMO_from_json.py
msgid "large pistol primer"
msgstr "spłonka do wysokokalibrowych pistoletów"

#. ~ Description for large pistol primer
#: lang/json/AMMO_from_json.py
msgid "Primer from a large caliber pistol round."
msgstr "Spłonka używana w nabojach pistoletowych dużego kalibru do inicjowania zapłonu ładunku właściwego."

#: lang/json/AMMO_from_json.py
msgid "small rifle primer"
msgstr "spłonka do niskokalibrowych karabinów"

#. ~ Description for small rifle primer
#: lang/json/AMMO_from_json.py
msgid "Primer from a small caliber rifle round."
msgstr "Spłonka używana w nabojach karabinowych niskiego kalibru do inicjowania zapłonu ładunku właściwego."

#: lang/json/AMMO_from_json.py
msgid "large rifle primer"
msgstr "spłonka do wysokokalibrowych karabinów"

#. ~ Description for large rifle primer
#: lang/json/AMMO_from_json.py
msgid "Primer from a large caliber rifle round."
msgstr "Spłonka używana w nabojach karabinowych dużego kalibru do inicjowania zapłonu ładunku właściwego."

#: lang/json/AMMO_from_json.py
msgid "rubber slug"
msgstr "gumowy pocisk"

#. ~ Description for rubber slug
#: lang/json/AMMO_from_json.py
msgid ""
"Rubber slugs from a shotgun beanbag round.  You could use them to make new "
"beanbag rounds."
msgstr "Gumowe pociski do produkcji specjalnych gumowych nabojów do strzelb. Możesz ich użyć do tworzenia nowych gumowych nabojów."

#: lang/json/AMMO_from_json.py
msgid "copper"
msgstr "miedź"

#. ~ Description for copper
#: lang/json/AMMO_from_json.py
msgid ""
"Copper scraps.  Could be used to craft something, for example makeshift "
"shotgun shells."
msgstr "Kawałki miedzi. Można ich użyć do wytworzenia czegoś, np. prowizorycznych loftek do strzelby."

#: lang/json/AMMO_from_json.py
msgid "solder"
msgstr "lut"

#. ~ Description for solder
#: lang/json/AMMO_from_json.py
msgid ""
"A small spool of solder, able to be used in ammunition and electronics."
msgstr "Mała zwitka metalu lutowniczego, stosowana w elektronice i produkcji amunicji."

#: lang/json/AMMO_from_json.py
msgid "tin"
msgstr "cyna"

#. ~ Description for tin
#: lang/json/AMMO_from_json.py
msgid "Tin and pewter scraps.  Usable in making ammo."
msgstr "Złom cyny i jej stopów. Przydatny do produkcji amunicji."

#: lang/json/AMMO_from_json.py
msgid "medical tape"
msgstr "taśma medyczna"

#. ~ Description for medical tape
#: lang/json/AMMO_from_json.py
msgid "A roll of medical tape, similar to duct tape."
msgstr "Zwój taśmy medycznej, podobnej nieco do taśmy montażowej."

#: lang/json/AMMO_from_json.py
msgid "FFV441B HE rocket"
msgstr "rakieta FFV441B typ HE"

#. ~ Description for FFV441B HE rocket
#: lang/json/AMMO_from_json.py
msgid ""
"A 84x246mm High Explosive anti-personnel round for the Carl Gustav M3 "
"recoilless rifle.  Designed to be highly effective against personnel."
msgstr "Rakieta 84x246mm typu HE - eksplodująca - przeciwpiechotna do wyrzutni bezodrzutowej Carl Gustav M3.  Zaprojektowana celem zmaksymalizowania efektywności przeciwko celom żywym."

#: lang/json/AMMO_from_json.py
msgid "FFV502 HEDP rocket"
msgstr "rakieta FFV441B typ HEDP"

#. ~ Description for FFV502 HEDP rocket
#: lang/json/AMMO_from_json.py
msgid ""
"A 84x246mm High Explosive Dual Purpose anti-materiel round for the Carl "
"Gustav M3 recoilless rifle.  Designed to be highly effective against "
"vehicles and structures."
msgstr "Rakieta 84x246mm typu HEDP - eksplodująca podwójnego przeznaczenia - do wyrzutni bezodrzutowej Carl Gustav M3.  Zaprojektowana przeciwko pojazdom i obiektom budowlanym."

#: lang/json/AMMO_from_json.py
msgid "FFV469 smoke rocket"
msgstr "rakieta FFV441B typ dymny"

#. ~ Description for FFV469 smoke rocket
#: lang/json/AMMO_from_json.py
msgid ""
"A 84x246mm smoke marker round for the Carl Gustav M3 recoilless rifle.  "
"Commonly used for signaling, target designation, and for screening troop "
"movements."
msgstr "Rakieta 84x246mm typu dymnego - do wyrzutni bezodrzutowej Carl Gustav M3.  Używana do sygnalizowania, wskazywania celu lub rozpościerania zasłon dymnych."

#: lang/json/AMMO_from_json.py
msgid "PG-7VL rocket"
msgstr "rakieta PG-7VL"

#. ~ Description for PG-7VL rocket
#: lang/json/AMMO_from_json.py
msgid "93mm single-stage high-explosive ammunition for the RPG-7."
msgstr "93 milimetrowy, jednoczęściowy, eksplodujący, rakietowo napędzany granat do RPG-7."

#: lang/json/AMMO_from_json.py
msgid "M235 TPA rocket"
msgstr "rakieta M235 TPA"

#. ~ Description for M235 TPA rocket
#: lang/json/AMMO_from_json.py
msgid ""
"66mm incendiary rockets used in the M202 FLASH rocket launcher.  Their "
"warheads are filled with a thickened pyrophoric agent, which burns at 2200 "
"degrees Fahrenheit."
msgstr "66 milimetrowe zapalające rakiety do wyrzutni  M202 FLASH. Posiadają głowicę wypełnioną zapalającym związkiem chemicznym płonącym w temperaturze 1200 stopni Celsjusza."

#: lang/json/AMMO_from_json.py
msgid "placeholder ammunition"
msgstr "puste miejsce amunicja"

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "charcoal"
msgstr "węgiel drzewny"

#. ~ Description for charcoal
#: lang/json/AMMO_from_json.py
msgid ""
"Charcoal is a flammable carbon based material obtained by removing water and"
" other volatile constituents from animal and vegetation substances.  It is "
"produced by slowly heating wood or other substances in the absence of "
"oxygen."
msgstr "Węgiel drzewny to palny węgiel pozyskany w wyniku odparowania wody i innych nietrwałych lotnych substancji z materiałów pochodzenia roślinnego lub zwierzęcego. Można go wyprodukować wolno ogrzewając drewno lub inne substancje w  warunkach beztlenowych. "

#: lang/json/AMMO_from_json.py
msgid "coal"
msgstr "węgiel"

#. ~ Description for coal
#: lang/json/AMMO_from_json.py
msgid ""
"Flammable black chunks of carbon-based material commonly used for cooking "
"and heating."
msgstr "Palne, czarne kawałki materiału opartego na węglu, często używane do gotowania i ogrzewania."

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "modified mininuke"
msgstr "zmodyfikowana miniatomówka"

#. ~ Description for modified mininuke
#: lang/json/AMMO_from_json.py
msgid ""
"A heavy modified handheld nuclear device.  Intended to be fired from a "
"specialized launcher, its case has been cut down and it has been rigged to "
"explode on impact.  While it is now somewhat lighter than before, it can no "
"longer be triggered manually."
msgstr "Walizkowy ładunek jądrowy znacznie zmodyfikowany w celu dostosowania go do użycia w wyspecjalizowanej wyrzutni. Wycięto go z pierwotnej obudowy i zamontowano zapalnik uderzeniowy. Nie można już ręcznie ustawić jego detonacji."

#: lang/json/AMMO_from_json.py
msgid "RA21E medical ampoule"
msgstr "ampułka medyczna RA21E"

#. ~ Description for RA21E medical ampoule
#: lang/json/AMMO_from_json.py
msgid ""
"A tiny glass vial filled with advanced fast-healing chemicals which can only"
" be activated by a Rivtech jet injector device.  A label on the side warns "
"against using more than two doses per hour."
msgstr "Mała szklana ampułka wypełniona zaawansowanymi związkami chemicznymi przyspieszającymi procesy gojenia, która może być użyta tylko z  wstrzykiwaczem Rivtech. Opis na naklejce przestrzega przed stosowaniem więcej niż dwóch dawek na godzinę."

#: lang/json/AMMO_from_json.py
msgid "RA10K stimulant module"
msgstr "RA10K moduł stymulatora"

#. ~ Description for RA10K stimulant module
#: lang/json/AMMO_from_json.py
msgid ""
"A pressurized multi-chamber glass vial filled with powerful chemical "
"stimulants which can only be activated by a Rivtech stimulant delivery "
"system."
msgstr "Ciśnieniowa wielokomorowa kapsuła szklana wypełniona chemicznymi stymulantami, które mogą być dozowane wyłącznie systemem dozowania stymulantów maki Rivtech."

#: lang/json/AMMO_from_json.py
msgid "rebreather filter cartridge"
msgstr "filtr do oddycharki"

#. ~ Description for rebreather filter cartridge
#: lang/json/AMMO_from_json.py
msgid "A replacement filter cartridge for a rebreather."
msgstr "Filtr zamienny do aparatu oddechowego z zamkniętym obiegiem powietrza."

#: lang/json/AMMO_from_json.py
msgid "nicotine liquid"
msgstr "płyn nikotynowy"

#. ~ Description for nicotine liquid
#: lang/json/AMMO_from_json.py
msgid ""
"A liquid that is made up of propylene glycol, vegetable glycerine, "
"flavorings and nicotine."
msgstr "Płyn składający się z glikolu propylenowego, glicerolu, aromatów i nikotyny."

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "fish bait"
msgstr "przynęta na ryby"

#. ~ Description for fish bait
#: lang/json/AMMO_from_json.py
msgid "A bait used in traps to lure fish."
msgstr "Przynęta używana w pułapkach do zwabiania ryb."

#: lang/json/AMMO_from_json.py
msgid "oxygen"
msgstr "tlen"

#. ~ Description for oxygen
#: lang/json/AMMO_from_json.py
msgid "A canister of oxygen."
msgstr "Zbiornik z tlenem."

#: lang/json/AMMO_from_json.py
msgid "spiked home-made rocket"
msgstr "domowa kolco-rakieta"

#. ~ Description for spiked home-made rocket
#: lang/json/AMMO_from_json.py
msgid ""
"A home-built rocket, consisting of a spike welded onto a pipe that was "
"filled with improvised rocket fuel.  Horribly inaccurate, as can be expected"
" of this grade of weapon, but packs a fierce punch... if it hits."
msgstr "Domowej roboty rakieta złożona z kolca przyspawanego na rurze, którą wypełniono improwizowanym paliwem rakietowym. Żałośnie niecelna, jak się można spodziewać po domowej produkcji, ale daje kopa ...o ile trafi."

#: lang/json/AMMO_from_json.py
msgid "explosive home-made rocket"
msgstr "domowa rakieta wybuchająca"

#. ~ Description for explosive home-made rocket
#: lang/json/AMMO_from_json.py
msgid ""
"A home-built rocket, consisting of a simple explosive warhead attached onto "
"a pipe, filled with improvised rocket fuel.  Not very powerful, but can be "
"made from scratch."
msgstr "Domowej roboty rakieta złożona z prostego ładunku wybuchowego zamocowanego na rurze, którą wypełniono improwizowanym paliwem rakietowym. Niezbyt silna, ale można ją zrobić z niczego."

#: lang/json/AMMO_from_json.py
msgid "incendiary home-made rocket"
msgstr "domowa rakieta zapalająca"

#. ~ Description for incendiary home-made rocket
#: lang/json/AMMO_from_json.py
msgid ""
"A home-built rocket, consisting of a container of flammable gel attached "
"onto a pipe, filled with improvised rocket fuel.  Short-range incendiary – "
"handle with care!"
msgstr "Domowej roboty rakieta złożona z pojemnika z palnym żelem zamocowanego na rurze, którą wypełniono improwizowanym paliwem rakietowym. Podpalanie krótkiego zasięgu, obchodzić się jak z jajkiem!"

#: lang/json/AMMO_from_json.py
msgid "unfinished charcoal"
msgstr "niewypalony węgiel drzewny"

#. ~ Description for unfinished charcoal
#: lang/json/AMMO_from_json.py
msgid ""
"The result of an interrupted charcoal making process.  Useless, except as "
"fuel in a charcoal kiln."
msgstr "Skutek przerwanego procesu wypalania węgla drzewnego. Bezużyteczny, chyba że jako paliwo do pieca węglowego."

#: lang/json/AMMO_from_json.py
msgid "sprayable fungicide"
msgstr "fungicyd w sprayu"

#. ~ Description for sprayable fungicide
#: lang/json/AMMO_from_json.py
msgid ""
"Fungicide enhanced with sulfur to create sprayable anti-fungal ammo for the "
"chemical thrower.  Best be used with some kind of mask or mouth protection."
msgstr "Preparat grzybobójczy zmieszany z siarką w celu uzyskania rozpylanej amunicji do miotacza chemicznego. Lepiej używać wraz z jakąś maską lub inną ochroną ust."

#: lang/json/AMMO_from_json.py
msgid "sprayable toxic gas"
msgstr "toksyczny gaz w sprayu"

#. ~ Description for sprayable toxic gas
#: lang/json/AMMO_from_json.py
msgid ""
"A hazardous mixture made from household cleaners, improvised for use in a "
"chemical thrower.  Best be used with some kind of mask or mouth protection."
msgstr "Niebezpieczna mieszanina domowych detergentów tworząca prowizoryczną amunicję do miotacza chemicznego. Lepiej używać wraz z jakąś maską lub inną ochroną ust."

#: lang/json/AMMO_from_json.py
msgid "arrowhead"
msgstr "grot strzały"

#. ~ Description for arrowhead
#: lang/json/AMMO_from_json.py
msgid ""
"Steel heads for an arrow.  Placing these on arrows will make them more "
"damaging and piercing."
msgstr "Stalowe groty do strzał. Zamocowanie ich na strzałach zwiększy ich obrażenia i przebicie."

#: lang/json/AMMO_from_json.py
msgid "wooden arrow shaft"
msgstr "drewniany promień strzały"

#. ~ Description for wooden arrow shaft
#: lang/json/AMMO_from_json.py
msgid ""
"A crude shaft made from wood.  It needs arrowheads and fletching to become a"
" functional arrow."
msgstr "Surowe drewniane promienie strzał. Wymagają zaopatrzenia w groty i lotki aby stały się pełnoprawnymi strzałami."

#: lang/json/AMMO_from_json.py
msgid "heavy wooden arrow shaft"
msgstr "ciężki drewniany promień strzały"

#. ~ Description for heavy wooden arrow shaft
#: lang/json/AMMO_from_json.py
msgid ""
"A heavy shaft made from wood.  It needs arrowheads and fletching to become a"
" functional arrow."
msgstr "Ciężkie drewniane promienie strzał. Wymagają zaopatrzenia w groty i lotki aby stały się pełnoprawnymi strzałami."

#: lang/json/AMMO_from_json.py
msgid "metal arrow shaft"
msgstr "metalowy promień strzały"

#. ~ Description for metal arrow shaft
#: lang/json/AMMO_from_json.py
msgid ""
"A heavy shaft made from scrap metal.  It needs arrowheads and fletching to "
"become a functional arrow."
msgstr "Ciężkie metalowe promienie strzał stworzone ze złomu. Wymagają zaopatrzenia w groty i lotki aby stały się pełnoprawnymi strzałami."

#: lang/json/AMMO_from_json.py
msgid "unfletched fire-hardened wooden arrow"
msgstr "bezlotkowa hartowana ogniem drewniana strzała"

#. ~ Description for unfletched fire-hardened wooden arrow
#: lang/json/AMMO_from_json.py
msgid ""
"A simple arrow shaft that has had a point carved into it and then fire-"
"hardened.  It needs fletching to be worth firing from a bow."
msgstr "Prosta strzała bez lotek z zaostrzonym promieniem hartowanym w ogniu. Wymaga zamontowania lotek."

#: lang/json/AMMO_from_json.py
msgid "unfletched field point wooden arrow"
msgstr "bezlotkowa szpicowana drewniana strzała"

#. ~ Description for unfletched field point wooden arrow
#: lang/json/AMMO_from_json.py
msgid ""
"A simple arrow shaft that has a crude arrowhead.  Useful for hunting small "
"woodland creatures, but needs fletching before it is worth firing."
msgstr "Prosta strzała bez lotek ze szpiczastym grotem. Przydatna do polowania na małe leśne zwierzęta, ale wymaga zamontowania lotek."

#: lang/json/AMMO_from_json.py
msgid "unfletched small game arrow"
msgstr "bezlotkowa szerokoszpica drewniana strzała"

#. ~ Description for unfletched small game arrow
#: lang/json/AMMO_from_json.py
msgid ""
"A simple arrow shaft that has a wide blunt head.  Useful for hunting small "
"woodland creatures without splattering them all over the ground, but needs "
"fletching before it is worth firing."
msgstr "Prosta strzała bez lotek z szerokim płaskim grotem. Przydatna do polowania na małe leśne zwierzęta bez rozsmarowywania ich po okolicy, ale wymaga zamontowania lotek."

#: lang/json/AMMO_from_json.py
msgid "fire-hardened wooden arrow"
msgstr "hartowana ogniem drewniana strzała z lotkami"

#. ~ Description for fire-hardened wooden arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This simple arrow has a fire-hardened point and fletching.  Stands a below "
"average chance of remaining intact once fired."
msgstr "Prosta strzała z lotami z zaostrzonym promieniem hartowanym w ogniu. Szansa, że pozostanie w jednym kawałku po wystrzeleniu jest poniżej przeciętnej."

#: lang/json/AMMO_from_json.py
msgid "field point wooden arrow"
msgstr "szpicowana drewniana strzała z lotkami"

#. ~ Description for field point wooden arrow
#: lang/json/AMMO_from_json.py
msgid ""
"A simple arrow shaft that has a crude arrowhead and fletching.  Useful for "
"hunting small woodland creatures or as a last ditch defense against zombies."
"  Stands a below average chance of remaining intact once fired."
msgstr "Prosta strzała z lotkami z zaostrzonym grotem. Dobra do polowania na małe leśne zwierzęta, i broń ostatniej szansy przeciw zombie. Szansa, że pozostanie w jednym kawałku po wystrzeleniu jest poniżej przeciętnej."

#: lang/json/AMMO_from_json.py
msgid "small game arrow"
msgstr "szerokoszpica drewniana strzała z lotkami"

#. ~ Description for small game arrow
#: lang/json/AMMO_from_json.py
msgid ""
"A simple arrow shaft that has a wide blunt head.  Useful for hunting small "
"woodland creatures without splattering them all over the ground, but stands "
"a below average chance of remaining intact once fired."
msgstr "Prosta strzała z lotkami lotek z szerokim płaskim grotem. Przydatna do polowania na małe leśne zwierzęta bez rozsmarowywania ich po okolicy, ale z wysoką szansą złamania po strzale."

#: lang/json/AMMO_from_json.py
msgid "wooden arrow"
msgstr "drewniana strzała z lotkami"

#. ~ Description for wooden arrow
#: lang/json/AMMO_from_json.py
msgid ""
"A basic wooden arrow, it has a metal arrowhead and fletching.  It's light-"
"weight, does some damage, and is so-so on accuracy.  Stands a below average "
"chance of remaining intact once fired."
msgstr "Podstawowa drewniana strzała z metalowym grotem i lotkami. Lekka, zadaje umiarkowane obrażenia i ma jako taką celność. Szansa, że pozostanie w jednym kawałku po wystrzeleniu jest poniżej przeciętnej."

#: lang/json/AMMO_from_json.py
msgid "heavy fire-hardened arrow"
msgstr "hartowana ogniem drewniana strzała z lotkami"

#. ~ Description for heavy fire-hardened arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This arrow is significantly sturdier than other wooden arrows.  It has a "
"fire-hardened point and fletching.  Stands a good chance of remaining intact"
" once fired."
msgstr "Ta strzała jest zdecydowanie wytrzymalsza od innych. Ma hartowany w ogniu szpic i lotki. Ma spore szanse pozostać w całości po wystrzeleniu."

#: lang/json/AMMO_from_json.py
msgid "unfletched heavy fire-hardened arrow"
msgstr "bezlotkowa ciężkia hartowana ogniem drewniana strzała"

#. ~ Description for unfletched heavy fire-hardened arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This arrow is significantly sturdier than other wooden arrows.  It has a "
"fire-hardened point, but needs fletching to be worth firing."
msgstr "Ta strzała jest zdecydowanie wytrzymalsza od innych. Ma hartowany w ogniu szpic ale przydałyby się jej lotki."

#: lang/json/AMMO_from_json.py
msgid "unfletched heavy field point arrow"
msgstr "bezlotkowa ciężkia szpicowana drewniana strzała"

#. ~ Description for unfletched heavy field point arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This arrow is significantly sturdier than other wooden arrows.  It has a "
"field point tip, but needs fletching before it is worth firing."
msgstr "Ta strzała jest zdecydowanie wytrzymalsza od innych. Ma szpiczasty grot ale przydałyby się jej lotki."

#: lang/json/AMMO_from_json.py
msgid "heavy field point arrow"
msgstr "szpicowana ciężka drewniana strzała"

#. ~ Description for heavy field point arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This arrow is significantly sturdier than other wooden arrows.  It has a "
"crude field point head and fletching.  Stands a good chance of remaining "
"intact once fired."
msgstr "Ta strzała jest zdecydowanie wytrzymalsza od innych. Ma szpiczasty grot i lotki. Ma spore szanse pozostać w całości po wystrzeleniu."

#: lang/json/AMMO_from_json.py
msgid "heavy wooden arrow"
msgstr "ciężki drewniana strzała"

#. ~ Description for heavy wooden arrow
#: lang/json/AMMO_from_json.py
msgid ""
"A heavy wooden arrow, with fletching and a metal arrowhead.  It's much "
"heavier than other wooden arrows, and as a result it does more damage and is"
" more stable in flight, resulting in better accuracy over a longer range.  "
"Stands a good chance of remaining intact once fired."
msgstr "Ciężka drewniana strzała z metalowym grotem i lotkami. Znacznie cięższa od innych strzał, powoduje zwiększone obrażenia i jest stabilniejsza w locie przez co ma lepszą celność na dalszych dystansach. Ma spore szanse pozostać w jednym kawałku po wystrzeleniu."

#: lang/json/AMMO_from_json.py
msgid "unfletched sharpened metal arrow"
msgstr "bezlotkowe zaostrzona metalowa strzała"

#. ~ Description for unfletched sharpened metal arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This metal shaft has been carefully sharpened so that it has a crude point "
"at the end.  It needs fletching to be worth firing from a bow."
msgstr "Metalowy promień tej strzały został uważnie zaostrzony na jednym z końców. Wymaga zamocowania lotek."

#: lang/json/AMMO_from_json.py
msgid "sharpened metal arrow"
msgstr "zaostrzona metalowa strzała"

#. ~ Description for sharpened metal arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This metal shaft has been carefully sharpened so that it has a crude point "
"at the end.  Stands a very good chance of remaining intact once fired."
msgstr "Metalowy promień tej strzały został uważnie zaostrzony na jednym z końców. Posiada lotki. Ma dużą szanse pozostać w całości po wystrzeleniu."

#: lang/json/AMMO_from_json.py
msgid "metal arrow"
msgstr "metalowa strzała"

#. ~ Description for metal arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This metal arrow has fletching and a steel arrowhead.  Stands a very good "
"chance of remaining intact once fired."
msgstr "Metalowa strzała z grotem i lotkami. Szanse na to, że można ją ponownie wykorzystać po wystrzeleniu, są bardzo wysokie."

#: lang/json/AMMO_from_json.py
msgid "carbon fiber arrow"
msgstr "strzała z włókna węglowego"

#. ~ Description for carbon fiber arrow
#: lang/json/AMMO_from_json.py
msgid ""
"High-tech carbon fiber shafts and 100 grain broadheads.  Very lightweight, "
"fast, and notoriously fragile.  Stands a bad chance of remaining intact once"
" fired."
msgstr "Zaawansowana technologicznie strzała z włókna poliwęglanowego z zamocowanym stugramowym szerokim stalowym grotem. Bardzo lekka i szybka, ale ciesząca się złą sławą z powodu kruchości. Szansa na ponowne użycie po wystrzeleniu jest niska."

#: lang/json/AMMO_from_json.py
msgid "exploding arrow"
msgstr "eksplodująca strzała"

#. ~ Description for exploding arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This arrow has a small IED strapped to its tip.  Horribly inaccurate, heavy,"
" and bulky, not only does this contraption require expert hands to pull off "
"a shot, it requires one to loose this with a small measure of confidence the"
" shooter won't be caught in the blast radius."
msgstr "Strzała ta ma przywiązany mały improwizowany ładunek wybuchowy w miejsce grota. Okropnie niecelna, źle wyważona i ciężka. Ustrojstwo to wymaga wyćwiczonej ręki by w ogóle oddać strzał, i dozy wiary od strzelca, że nie znajdzie się w promieniu wybuchu."

#: lang/json/AMMO_from_json.py
msgid "flaming arrow"
msgstr "ognista strzała"

#. ~ Description for flaming arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This arrow has a flaming rag wrapped around the shaft near the head.  You "
"should shoot it soon before it burns your bow."
msgstr "Ta strzała ma płonącą szmatę przewiązaną wokół grota. Wystrzel zanim spali ci łuk."

#: lang/json/AMMO_from_json.py
msgid "wooden crossbow bolt"
msgstr "drewniany bełt"

#. ~ Description for wooden crossbow bolt
#: lang/json/AMMO_from_json.py
msgid ""
"A sharpened bolt carved from wood.  It's very light, but doesn't do much "
"damage and isn't particularly accurate.  Stands a below average chance of "
"remaining intact once fired."
msgstr "Zaostrzony bełt wystrugany z drewna. Bardzo lekki, ale nie zadaje dużych obrażeń i nie jest specjalnie celny. Szansa, że pozostanie w jednym kawałku po wystrzeleniu jest poniżej przeciętnej."

#: lang/json/AMMO_from_json.py
msgid "metal crossbow bolt"
msgstr "metalowy bełt"

#. ~ Description for metal crossbow bolt
#: lang/json/AMMO_from_json.py
msgid ""
"A sharpened bolt made from metal of some kind.  It's heavy and unwieldy, "
"providing moderate damage and accuracy.  Stands a very good chance of "
"remaining intact once fired."
msgstr "Ostry bełt zrobiony z jakiegoś metalu. Ciężki i nieporęczny, ale zadaje umiarkowane obrażenia i jest celny. Ma spore szanse pozostać w całości po wystrzeleniu."

#: lang/json/AMMO_from_json.py
msgid "steel crossbow bolt"
msgstr "stalowy bełt"

#. ~ Description for steel crossbow bolt
#: lang/json/AMMO_from_json.py
msgid ""
"A sharp bolt made from steel.  Deadly in skilled hands.  Stands an excellent"
" chance of remaining intact once fired."
msgstr "Ostry bełt zrobiony ze stali. Ma doskonałe szanse pozostać w całości po strzale."

#: lang/json/AMMO_from_json.py
msgid "explosive crossbow bolt"
msgstr "eksplodujący bełt"

#. ~ Description for explosive crossbow bolt
#: lang/json/AMMO_from_json.py
msgid ""
"A lightweight, sharpened bolt made from some kind of metal.  It is fairly "
"unwieldy and only provides moderate accuracy, due to being filled with "
"explosives and fitted with an impact trigger."
msgstr "Lekkie zaostrzone bełty z metalu. Dość nieporęczne i o wątpliwej celności z uwagi na zamontowane ładunki wybuchowe z zapalnikiem uderzeniowym."

#: lang/json/AMMO_from_json.py
msgid "plastic shaft"
msgstr "plastikowy promień strzały"

#. ~ Description for plastic shaft
#: lang/json/AMMO_from_json.py
msgid ""
"A crude shaft made from plastic.  It needs arrowheads and fletching to "
"become a functional arrow."
msgstr "Surowy promień strzały z plastiku. Potrzebuje grotu i lotek by stać się pełnoprawną strzałą."

#: lang/json/AMMO_from_json.py
msgid "plastic arrowhead"
msgstr "plastikowy grot strzały"

#. ~ Description for plastic arrowhead
#: lang/json/AMMO_from_json.py
msgid ""
"Plastic heads for an arrow.  Placing these on arrows will make them more "
"damaging and piercing."
msgstr "Plastikowe groty do strzał. Zamocowane na strzałach zwiększają ich obrażenia i przebicie."

#: lang/json/AMMO_from_json.py
msgid "throwing knife"
msgid_plural "throwing knives"
msgstr[0] "nóż do rzucania"
msgstr[1] "noże do rzucania"
msgstr[2] "noże do rzucania"
msgstr[3] "noże do rzucania"

#. ~ Description for throwing knife
#: lang/json/AMMO_from_json.py
msgid ""
"A thin and flat knife made for throwing.  Its ineffective cutting edge and "
"odd shape makes it unsuitable for use as a tool."
msgstr "Wyważony cienki i płaski nóż z jednego kawałka stali stworzony do rzucania. Nie nadaje się jako narzędzie z uwagi na kształt i brak typowego ostrza."

#: lang/json/AMMO_from_json.py
msgid "throwing axe"
msgstr "topór do rzucania"

#. ~ Description for throwing axe
#: lang/json/AMMO_from_json.py
msgid ""
"A lightweight hatchet made for throwing.  Its ineffective cutting edge and "
"light weight makes it unsuitable for use as a tool."
msgstr "Lekki toporek do rzucania. Nie nadaje się jako narzędzie z uwagi na kształt i brak typowego ostrza."

#: lang/json/AMMO_from_json.py
msgid "lawn dart"
msgstr "strzałka do gry"

#. ~ Description for lawn dart
#: lang/json/AMMO_from_json.py
msgid "A large plastic dart made for outdoor games."
msgstr "Duża plastikowa strzałka do gry na świeżym powietrzu."

#: lang/json/AMMO_from_json.py
msgid "wooden fishing spear"
msgstr "drewniana włócznia wędkarska"

#. ~ Description for wooden fishing spear
#: lang/json/AMMO_from_json.py
msgid ""
"An underwater fishing spear made from wood tipped with steel.  It's very "
"light, but doesn't have much range.  Stands a below average chance of "
"remaining intact once fired."
msgstr "Włócznia do podwodnego połowu ryb wykonana z drewna zwieńczonego stalowym ostrzem. Jest bardzo lekka, ale ma niewielki zasięg. Ma niedużą szansę pozostać w całości po wystrzeleniu."

#: lang/json/AMMO_from_json.py
msgid "metal fishing spear"
msgstr "metalowa włócznia wędkarska"

#. ~ Description for metal fishing spear
#: lang/json/AMMO_from_json.py
msgid ""
"An underwater fishing spear made from metal.  It's light, but doesn't have "
"much range.  Stands a very good chance of remaining intact once fired."
msgstr "Włócznia do podwodnego połowu ryb wykonana z metalu. Jest lekka, ale ma niewielki zasięg. Ma sporą szansę pozostać w całości po wystrzeleniu."

#: lang/json/AMMO_from_json.py
msgid "carbon fiber fishing spear"
msgstr "włócznia wędkarskia z włókna węglowego"

#. ~ Description for carbon fiber fishing spear
#: lang/json/AMMO_from_json.py
msgid ""
"An underwater fishing spear made from carbon fiber.  It's very light, but "
"doesn't have much range.  Stands a bad chance of remaining intact once "
"fired."
msgstr "Włócznia do podwodnego połowu ryb wykonana z włókien węglowych. Jest bardzo lekka, ale ma niewielki zasięg. Ma słabą szansę pozostać w całości po wystrzeleniu."

#: lang/json/AMMO_from_json.py
msgid "plastic arrow"
msgstr "plastikowa strzała"

#. ~ Description for plastic arrow
#: lang/json/AMMO_from_json.py
msgid ""
"This plastic arrow has a plastic arrowhead and fletching.  Stands a good "
"chance of remaining intact once fired."
msgstr "Ta plastikowa strzała ma plastikowy grot i lotki. Jej szansa na pozostanie w jednym kawałku po wystrzeleniu jest dobra."

#: lang/json/AMMO_from_json.py
msgid "sulfur"
msgid_plural "sulfur"
msgstr[0] "jednostka siarki"
msgstr[1] "jedn. siarki"
msgstr[2] "jedn. siarki"
msgstr[3] "siarka"

#. ~ Description for sulfur
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of yellow grains of pure sulfur.  Primarily used to make battery "
"acid, sulfur is sometimes used as fuel for explosives.  Can be burned to "
"produce acidic smoke, that is deadly to bacteria and humans alike, or "
"oxidized to make paper-bleaching agents."
msgstr "Garstka żółtych ziaren czystej siarki. Głównie używana do produkcji kwasu siarkowego, ale czasami jest stosowana jako paliwo do materiałów wybuchowych. Może być spalana w celu produkcji kwaśnego dymu, który jest zabójczy zarówno dla bakterii jak i dla ludzi, lub utleniona w celu produkcji wybielaczy do papieru."

#: lang/json/AMMO_from_json.py
msgid "cement"
msgid_plural "cement"
msgstr[0] "cement"
msgstr[1] "cement"
msgstr[2] "cement"
msgstr[3] "cement"

#. ~ Description for cement
#: lang/json/AMMO_from_json.py
msgid ""
"Portland is most likely long gone, but its memory persists in cement.  This "
"ubiquitous binder can be used for all kinds of advanced masonry.  Just add "
"water."
msgstr "Portlandzki zapewne już dawno się skończył, ale pamięć o nim jest cementowa. To powszechne spoiwo może być użyte w dowolnych rodzajach prac murarskich. Wystarczy dodać wodę."

#: lang/json/AMMO_from_json.py
msgid "limestone"
msgid_plural "limestone"
msgstr[0] "wapień"
msgstr[1] "wapień"
msgstr[2] "wapień"
msgstr[3] "wapień"

#. ~ Description for limestone
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of small chunks of limestone.  Useful for experiments at science "
"fairs, but pretty useless as ammo.  Maybe something could be crafted from "
"this stuff…"
msgstr "Kawałki wapienia. Używany do eksperymentów w dniu otwartym chemii, ale bezużyteczny jako amunicja. Może dałoby się coś z tego stworzyć..."

#: lang/json/AMMO_from_json.py
msgid "quicklime"
msgid_plural "quicklime"
msgstr[0] "wapno palone"
msgstr[1] "wapno palone"
msgstr[2] "wapno palone"
msgstr[3] "wapno palone"

#. ~ Description for quicklime
#: lang/json/AMMO_from_json.py
msgid ""
"The product of burning limestone, this white powder is a crucial ingredient "
"in making cement.  That said, it is also extremely caustic and will cause "
"severe burns to any tissue it comes in contact with.  This property could "
"probably be exploited..."
msgstr "Produkt wypalania wapienia. Ten biały proszek jest niezbędną składową cementu. Ponadto jest silnie zasadowy i spowoduje poważne poparzenia tkanek które wejdą z nim w kontakt. Tę właściwość można by wykorzystać..."

#: lang/json/AMMO_from_json.py lang/json/terrain_from_json.py
msgid "sand"
msgid_plural "sand"
msgstr[0] "piasek"
msgstr[1] "piasek"
msgstr[2] "piasek"
msgstr[3] "piasek"

#. ~ Description for sand
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of New England sand.  If you had a stoked furnace, you could turn "
"it into glass.  Otherwise, it's only good for making cement."
msgstr "Garść piasku Nowej Anglii.  W odpowiednim piecu stopiłby się w szkło. W przeciwnym wypadku nadaje się tylko do produkcji cementu."

#: lang/json/AMMO_from_json.py
msgid "aluminum powder"
msgid_plural "aluminum powder"
msgstr[0] "sproszkowane aluminium"
msgstr[1] "sproszkowane aluminium"
msgstr[2] "sproszkowane aluminium"
msgstr[3] "sproszkowane aluminium"

#. ~ Description for aluminum powder
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of finely ground aluminum powder.  This strong reducing agent was "
"used in combination with various oxidants for chemical welding and "
"explosives alike.  Just before the Cataclysm, however, it was the basis of "
"virtually all commercial metallic-hued paints and anti-corrosive coatings."
msgstr "Garść dobrze zmielonego proszku aluminiowego. Ten silny reduktor jest używany w wielu rożnych utleniaczach oraz do celów spawalniczych i w produkcji materiałów wybuchowych. Tuż przez Kataklizmem służył do wytwarzania niemal wszystkich komercyjnych metalizowanych farb i powłok antykorozyjnych."

#: lang/json/AMMO_from_json.py
msgid "zinc powder"
msgid_plural "zinc powder"
msgstr[0] "sproszkowany cynk"
msgstr[1] "sproszkowany cynk"
msgstr[2] "sproszkowany cynk"
msgstr[3] "sproszkowany cynk"

#. ~ Description for zinc powder
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of zinc powder.  While commercially used mostly in cosmetics and "
"for paints, hobbyists used to mix it with sulfur to make basic rocket fuel /"
" smokescreen.  When treated with a strong acid, it will produce hydrogen."
msgstr "Garść sproszkowanego cynku. Komercyjnie używany w przemyśle kosmetycznym i farbiarstwie, a hobbystycznie po zmieszaniu z siarka jako paliwo rakietowe lub zasłona dymna. Potraktowany silnym kwasem wytworzy wodór."

#: lang/json/AMMO_from_json.py
msgid "sulfuric acid"
msgid_plural "sulfuric acid"
msgstr[0] "kwas siarkowy"
msgstr[1] "kwas siarkowy"
msgstr[2] "kwas siarkowy"
msgstr[3] "kwas siarkowy"

#. ~ Description for sulfuric acid
#: lang/json/AMMO_from_json.py
msgid ""
"Sulfuric acid: viscous, foul-smelling and extremely corrosive, particularly "
"to organic matter and evil clowns.  Although used to prepare a number of "
"chemicals, it is mostly known as battery acid, due to its widest commercial "
"application."
msgstr "Kwas siarkowy to lepki, śmierdzący, gryzący i silnie korodujący kwas, szczególnie w stosunku do materii organicznej i złych klownów. Choć stosowany też w produkcji szeregu chemikaliów, w tym kwasu do baterii, i wielu innych zastosowań komercyjnych."

#: lang/json/AMMO_from_json.py
msgid "acetone"
msgid_plural "acetone"
msgstr[0] "aceton"
msgstr[1] "aceton"
msgstr[2] "aceton"
msgstr[3] "aceton"

#. ~ Description for acetone
#: lang/json/AMMO_from_json.py
msgid ""
"Acetone, best known as the primary constituent of most nail polish removers."
"  Flammable and can be easily used for potent home-made explosives, as "
"demonstrated by the terrorist attacks of 2016."
msgstr "Aceton, znany powszechnie rozpuszczalnik do lakieru do paznokci. Łatwopalny i prosty w użyciu w domowej produkcji materiałów wybuchowych, co pokazali terroryści w 2016 r."

#: lang/json/AMMO_from_json.py
msgid "nitric acid"
msgid_plural "nitric acid"
msgstr[0] "kwas azotowy"
msgstr[1] "kwas azotowy"
msgstr[2] "kwas azotowy"
msgstr[3] "kwas azotowy"

#. ~ Description for nitric acid
#: lang/json/AMMO_from_json.py
msgid ""
"Nitric acid, a strong oxidant and extremely corrosive material.  Mainly used"
" in the production of synthetic fertilizers, for etching of circuit boards "
"and in woodworking.  Remains quite useful both for making a vast array of "
"pyrotechnic and explosive agents and as a direct weapon: not many beings "
"enjoy being doused in nitric acid."
msgstr "Kwas azotowy to silny utleniacz i bardzo żrąca substancja. Najczęściej stosowany do produkcji syntetycznych nawozów, grawerowania, i wytwarzania gamy materiałów pirotechnicznych i wybuchowych. Da się zastosować jako broń bezpośrednia gdyż niewiele istot była by zadowolona z bycia oblanym tym kwasem."

#: lang/json/AMMO_from_json.py
msgid "chromium oxide"
msgid_plural "chromium oxide"
msgstr[0] "tlenek chromu"
msgstr[1] "tlenek chromu"
msgstr[2] "tlenek chromu"
msgstr[3] "tlenek chromu"

#. ~ Description for chromium oxide
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of chromium oxide pellets.  This substance, typically known as the"
" only stable green pigment for paints, is also a mild oxidizer, which can be"
" mixed with aluminum powder to make thermite for welding.  If one had access"
" to a CVD machine and a laboratory worth of reagents, they might be able to "
"produce very durable ceramic layers."
msgstr "Garść grudek tlenku chromu. Substancja ta jest znana jako jedyny stabilny zielony pigment do farb. Jest też umiarkowanym utleniaczem, a zmieszana ze sproszkowanym aluminium daje termit do spawania. Ktoś z dostępem do maszyny CVD i laboratorium pełnego składników mógłby stworzyć bardzo trwałe powłoki ceramiczne."

#: lang/json/AMMO_from_json.py
msgid "calcium carbide"
msgid_plural "calcium carbide"
msgstr[0] "acetylenek wapnia"
msgstr[1] "acetylenek wapnia"
msgstr[2] "acetylenek wapnia"
msgstr[3] "acetylenek wapnia"

#. ~ Description for calcium carbide
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of calcium carbide chunks, giving off a faint, garlic-like smell."
"  Commercially, calcium carbide was used for on-the-spot manufacture of "
"acetylene and historically to power acetylene lamps.  If mixed with water "
"and sealed, it will explode with little strength, but with relatively loud "
"noise and could maybe be used as a decoy..."
msgstr "Garść odłamków acetylenku wapnia, o lekkim zapachu czosnku. Komercyjnie wykorzystywany do wytwarzania acetylenu w miejscu zapotrzebowania na ten gaz, i do zasilania weń lamp gazowych. Wymieszany z wodą w zamkniętym pojemniku wybucha głośno lecz z małą siłą, więc może posłużyć jako przynęta."

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "hexamine"
msgid_plural "hexamine"
msgstr[0] "urotropina"
msgstr[1] "urotropina"
msgstr[2] "urotropina"
msgstr[3] "urotropina"

#. ~ Description for hexamine
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of waxy hexamine tablets.  This ubiquitous fuel was used to start "
"up almost every garden grill and no 'real' camping trip could do its cooking"
" without it, until the Cataclysm.  Nowadays, it is even more valuable then "
"before, being the key component in the preparation of several military-grade"
" explosives."
msgstr "Garść woskowych tabletek urotropiny. Powszechna rozpałka do grilla, bez której żadna prawdziwa wyprawa w dzicz nie mogła by się obyć, aż do Kataklizmu. Obecnie nawet bardziej wartościowa niż poprzednio, gdyż jest kluczowym komponentem kilku materiałów wybuchowych wojskowego poziomu."

#: lang/json/AMMO_from_json.py
msgid "hydrogen peroxide (concentrated)"
msgid_plural "hydrogen peroxide (concentrated)"
msgstr[0] "skoncentrowany nadtlenek wodoru"
msgstr[1] "skoncentrowany nadtlenek wodoru"
msgstr[2] "skoncentrowany nadtlenek wodoru"
msgstr[3] "skoncentrowany nadtlenek wodoru"

#. ~ Description for hydrogen peroxide (concentrated)
#: lang/json/AMMO_from_json.py
msgid ""
"Concentrated hydrogen peroxide is a powerful oxidant, violently decomposing "
"in contact with most reducing agents.  Can be used as rocket fuel or to make"
" high explosives.  Causes severe burns to any organic matter and may ignite "
"it on contact."
msgstr "Skoncentrowany nadtlenek wodoru to silny utleniacz, gwałtowanie reagujący w kontakcie z większością reduktorów. Może być użyty jako paliwo rakietowe lub w produkcji materiałów wybuchowych. Powoduje poważne poparzenia w kontakcie z materią organiczną i może ją nawet podpalić."

#: lang/json/AMMO_from_json.py
msgid "ammonium nitrate"
msgid_plural "ammonium nitrate"
msgstr[0] "azotan amonu"
msgstr[1] "azotan amonu"
msgstr[2] "azotan amonu"
msgstr[3] "azotan amonu"

#. ~ Description for ammonium nitrate
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of ammonium nitrate.  This white, crystalline powder is commonly "
"used as a component of fertilizers and explosives.  Could also be dissolved "
"in water to sharply lower its temperature, but is there any use for this "
"quality in this new world?"
msgstr "Garść azotanu amonu. Ten biały krystaliczny proszek jest powszechnie wykorzystywany w produkcji nawozów i materiałów wybuchowych. Rozpuszczony w wodzie gwałtownie ją ochładza, ale jak to wykorzystać w tym nowym świecie? "

#: lang/json/AMMO_from_json.py
msgid "saltpeter"
msgid_plural "saltpeter"
msgstr[0] "azotan potasu"
msgstr[1] "azotan potasu"
msgstr[2] "azotan potasu"
msgstr[3] "azotan potasu"

#. ~ Description for saltpeter
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of saltpeter.  Sometimes used as a fertilizer, this ubiquitous "
"nitrate is the principal constituent of black gunpowder and simple rocket "
"propellants, such as rocket candy."
msgstr "Garść azotanu potasu. Czasem używany jako nawóz, ten powszechny azotan jest podstawowym składnikiem prochu strzelniczego i prostych paliw rakietowych."

#: lang/json/AMMO_from_json.py
msgid "aluminum sulfate"
msgid_plural "aluminum sulfate"
msgstr[0] "siarczan glinu"
msgstr[1] "siarczan glinu"
msgstr[2] "siarczan glinu"
msgstr[3] "siarczan glinu"

#. ~ Description for aluminum sulfate
#: lang/json/AMMO_from_json.py
msgid ""
"Several crystals of aluminum sulfate.  Historically, an agent for purifying "
"water and as molluscicide, due to the fact that it degenerates their slime."
"  Just before the Cataclysm, its main uses were for adjusting the acidity of"
" the soil and as an additive to baking powder.  If mixed with a strong acid,"
" it could be used to saturate the soil with aluminum, causing all but the "
"most resistant of plants to wither and die."
msgstr "Kilkanaście kryształków siarczanu glinu. Historycznie środek oczyszczający wodę i ślimakobójczy, z uwagi na to że rozkłada ich śluz. Przed Kataklizmem stosowany do regulacji kwasowości gleby i jako dodatek do proszku do pieczenia. Zmieszany z silnym kwasem może wytrącić w glebie aluminium powodując śmierć większości roślin."

#: lang/json/AMMO_from_json.py
msgid "acetic acid"
msgid_plural "acetic acid"
msgstr[0] "kwas octowy"
msgstr[1] "kwas octowy"
msgstr[2] "kwas octowy"
msgstr[3] "kwas octowy"

#. ~ Description for acetic acid
#: lang/json/AMMO_from_json.py
msgid ""
"Concentrated acetic acid, typically used as a chemical reagent and "
"antifungal agent.  Despite its horrid smell, it was used to prepare several "
"types of perfumes, but would making perfume not be too ...fancy for "
"postapocalyptic New England?"
msgstr "Skoncentrowany kwas octowy stosowany jest w przemyśle chemicznym oraz jako środek grzybobójczy. Pomimo okropnego zapachu, stosowany był w produkcji perfum, ale czy ich wytwarzanie nie byłoby zbytkiem w postapokaliptycznej Nowej Anglii?"

#: lang/json/AMMO_from_json.py
msgid "thermite"
msgid_plural "thermite"
msgstr[0] "termit"
msgstr[1] "termit"
msgstr[2] "termit"
msgstr[3] "termit"

#. ~ Description for thermite
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of silvery-greenish powder, produced by mixing aluminum and "
"chromium oxide.  This mixture, burning at a constant pace and at extremely "
"high temperature, was once used for on-the-spot welding.  Can be used as an "
"incendiary material in a pinch."
msgstr "Garść srebrnozielonego proszku, powstałego ze zmieszanego sproszkowanego aluminium i i tlenku chromu. Mikstura ta, paląca się stabilnym płomieniem w olbrzymiej temperaturze stosowana była w miejscowym spawaniu elementów metalowych. W razie konieczności może być wykorzystana jako materiał zapalający."

#: lang/json/AMMO_from_json.py
msgid "ANFO"
msgid_plural "ANFO"
msgstr[0] "ANFO"
msgstr[1] "ANFO"
msgstr[2] "ANFO"
msgstr[3] "ANFO"

#. ~ Description for ANFO
#: lang/json/AMMO_from_json.py
msgid ""
"Several brownish pellets that were made by absorbing organic fuel on "
"ammonium nitrate.  This ammonium nitrate–fuel oil or ANFO, as it is called, "
"mixture is a common industrial explosive due to its stability and low price."
"  Burns fast, but needs an explosive primer to detonate..."
msgstr "Kilkanaście brązowawych granulek powstałych przez nasączenie paliwem organicznym azotanu amonu. Jest to przemysłowy materiał wybuchowy zwany ANFO lub saletrol, powszechny z uwagi na stabilność i niską cenę produkcji. Łatwopalny, ale wymaga zapalnika do zdetonowania."

#: lang/json/AMMO_from_json.py
msgid "black gunpowder"
msgid_plural "black gunpowder"
msgstr[0] "czarny proch strzelniczy"
msgstr[1] "czarny proch strzelniczy"
msgstr[2] "czarny proch strzelniczy"
msgstr[3] "czarny proch strzelniczy"

#. ~ Description for black gunpowder
#: lang/json/AMMO_from_json.py
msgid ""
"A handful of black gunpowder, made by mixing charcoal and sulfur with "
"saltpeter.  Pretty useless for making modern cartridges, as the soot "
"produced when it burns will quickly clog any firearm, but it could be used "
"to make some vicious bombs."
msgstr "Garść czarnego prochu, wytwarzanego przez mieszanie węgla i siarki z saletrą. Nie nadaje się do wytwarzania współczesnej amunicji, ponieważ sadza powstająca podczas spalania szybko zatkałaby każdą broń palną. Można go jednak użyć do wytwarzania niszczycielskich bomb."

#: lang/json/AMMO_from_json.py
msgid "RDX"
msgid_plural "RDX"
msgstr[0] "RDX"
msgstr[1] "RDX"
msgstr[2] "RDX"
msgstr[3] "RDX"

#. ~ Description for RDX
#: lang/json/AMMO_from_json.py
msgid ""
"Several off-white crystals of RDX or hexogen--a military-grade, shelf-stable"
" high explosive.  Can be cast into shaped charges or mixed with scrap to "
"pepper the blast area with high-velocity shrapnel."
msgstr "Kilkanaście białawych kryształków RDX zwanego hexogenem, czyli wojskowego materiału wybuchowego stabilnego w warunkach zwykłego składowania. Może być kształtowany w uformowane ładunki lub zmieszany ze złomem w celu zasypania obszaru szrapnelami o dużej prędkości."

#: lang/json/AMMO_from_json.py
msgid "HMTD"
msgid_plural "HMTD"
msgstr[0] "HMTD"
msgstr[1] "HMTD"
msgstr[2] "HMTD"
msgstr[3] "HMTD"

#. ~ Description for HMTD
#: lang/json/AMMO_from_json.py
msgid ""
"Hexamine peroxide: this fine, yellowish powder is a powerful primary "
"explosive, used to initiate detonation of more stable explosives, such as "
"RDX.  Relatively stable, but best not store it for too long..."
msgstr "Nadtlenek urotropiny: ten żółtawy proszek to silny detonator podstawowy dla bardziej stabilnych materiałów wybuchowym, takich jak RDX. Raczej stabilny, ale lepiej zbyt długo nie przechowywać."

#: lang/json/AMMO_from_json.py
msgid "rocket fuel"
msgid_plural "rocket fuel"
msgstr[0] "paliwo rakietowe"
msgstr[1] "paliwo rakietowe"
msgstr[2] "paliwo rakietowe"
msgstr[3] "paliwo rakietowe"

#. ~ Description for rocket fuel
#: lang/json/AMMO_from_json.py
msgid ""
"A chunk of a zinc powder and sulfur mixture, bound together by superglue, "
"that can be used as an impromptu rocket fuel."
msgstr "Kawałek sproszkowanego cynku zmieszanego z siarką, scalonego superglue, który może posłużyć jako improwizowane paliwo rakietowe."

#: lang/json/AMMO_from_json.py
msgid "detergent"
msgstr "detergent"

#. ~ Description for detergent
#: lang/json/AMMO_from_json.py
msgid "A popular pre-cataclysm washing powder."
msgstr "Popularny przed kataklizmem proszek do prania."

#: lang/json/AMMO_from_json.py
msgid "fungal seed"
msgstr "zarodniki grzybów"

#. ~ Description for fungal seed
#: lang/json/AMMO_from_json.py
msgid "Some fungal seeds."
msgstr "Trochę zarodników grzybów."

#: lang/json/AMMO_from_json.py lang/json/furniture_from_json.py
#: lang/json/furniture_from_json.py
msgid "fungal flower"
msgstr "grzybiczy kwiat"

#: lang/json/AMMO_from_json.py
msgid "ethanol"
msgid_plural "ethanol"
msgstr[0] "etanol"
msgstr[1] "etanol"
msgstr[2] "etanol"
msgstr[3] "etanol"

#. ~ Description for ethanol
#: lang/json/AMMO_from_json.py
msgid ""
"Extremely pure alcohol suitable for use as a component in chemical "
"reactions.  You could also water it down and drink it, or use it as fuel in "
"alcohol-burning stoves."
msgstr "Wyjątkowo czysty alkohol nadający się jako składnik reakcji chemicznych. Możesz go rozwodnić do picia, lub użyć w kuchenkach turystycznych."

#: lang/json/AMMO_from_json.py
msgid "denatured alcohol"
msgstr "denaturat"

#. ~ Description for denatured alcohol
#: lang/json/AMMO_from_json.py
msgid ""
"A high-strength ethanol solution mixed with methanol to make it toxic to "
"drink, so as to avoid pre-apocalyptic regulations on ethanol.  Intended for "
"use in alcohol-burning stoves and as a solvent."
msgstr "Wysokiej mocy roztwór etanolu i metanolu dla spowodowania niezdatności do spożycia, celem ominięcia regulacji prawnych dotyczących etanolu funkcjonujących przed kataklizmem. Przeznaczony do kuchenek turystycznych i jako rozpuszczalnik."

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "diesel"
msgid_plural "diesel"
msgstr[0] "diesel"
msgstr[1] "diesel"
msgstr[2] "diesel"
msgstr[3] "diesel"

#. ~ Description for diesel
#: lang/json/AMMO_from_json.py
msgid ""
"Petroleum-based fuel made from a byproduct of the gasoline making process."
msgstr "Olej napędowy typu diesel. Jest to produkt rafinacji ropy naftowej."

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "gasoline"
msgid_plural "gasoline"
msgstr[0] "benzyna"
msgstr[1] "benzyna"
msgstr[2] "benzyna"
msgstr[3] "benzyna"

#. ~ Description for gasoline
#: lang/json/AMMO_from_json.py
msgid ""
"Gasoline is a highly flammable liquid.  When under pressure, it has the "
"potential for violent explosion."
msgstr "Benzyna jest wysoce palnym płynem. Pod odpowiednim ciśnieniem ma potencjał do wytworzenia gwałtownych wybuchów."

#: lang/json/AMMO_from_json.py lang/json/ammunition_type_from_json.py
msgid "lamp oil"
msgid_plural "lamp oil"
msgstr[0] "nafta"
msgstr[1] "nafta"
msgstr[2] "nafta"
msgstr[3] "nafta"

#. ~ Description for lamp oil
#: lang/json/AMMO_from_json.py
msgid "A thin and clean-burning oil made for use in oil lamps."
msgstr "Lekkie, płonące czystym płomieniem paliwo do lamp."

#: lang/json/AMMO_from_json.py
msgid "napalm"
msgid_plural "napalm"
msgstr[0] "napalm"
msgstr[1] "napalm"
msgstr[2] "napalm"
msgstr[3] "napalm"

#. ~ Description for napalm
#: lang/json/AMMO_from_json.py
msgid ""
"This is gasoline mixed with some other components to make it stick to the "
"surface and increase burning temperature, meant to be used with a "
"flamethrower."
msgstr "Benzyna zmieszana z innymi dodatkami powodującymi jej przyleganie do różnych powierzchni i zwiększającymi jej temperaturę spalania, przeznaczona do miotaczy ognia."

#: lang/json/AMMO_from_json.py lang/json/terrain_from_json.py
msgid "concrete"
msgid_plural "concrete"
msgstr[0] "beton"
msgstr[1] "beton"
msgstr[2] "beton"
msgstr[3] "beton"

#. ~ Description for concrete
#: lang/json/AMMO_from_json.py
msgid "Some concrete, ready to be used in a construction project."
msgstr "Beton, gotowy do użycia w projektach budowlanych."

#: lang/json/AMMO_from_json.py
msgid "mortar"
msgid_plural "mortar"
msgstr[0] "zaprawa murarska"
msgstr[1] "zaprawa murarska"
msgstr[2] "zaprawa murarska"
msgstr[3] "zaprawa murarska"

#. ~ Description for mortar
#: lang/json/AMMO_from_json.py
msgid "Some mortar, ready to be used in building projects."
msgstr "Zaprawa murarska, gotowa do użycia w projektach budowlanych."

#: lang/json/AMMO_from_json.py
