msgstr[1] "suszony mięśniak"
msgstr[2] "suszony mięśniak"
msgstr[3] "suszony mięśniak"

#. ~ Description for jerk jerky
#: lang/json/COMESTIBLE_from_json.py
msgid ""
"Salty dried human flesh that never goes bad, but will make you thirsty."
msgstr "Solone i suszone mięso ludzkie, które się nie psuje ale wzmaga pragnienie."

#: lang/json/COMESTIBLE_from_json.py
msgid "smoked meat"
msgstr "wędzone mięso"

#. ~ Description for smoked meat
#: lang/json/COMESTIBLE_from_json.py
msgid "Tasty meat that has been heavily smoked for long term preservation."
msgstr "Smaczne mięso mocno uwędzone dla długotrwałego przechowywania."

#: lang/json/COMESTIBLE_from_json.py
msgid "smoked fish"
