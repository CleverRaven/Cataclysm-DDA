#: src/player_hardcoded_effects.cpp
#, c-format
msgid "You use your %s to keep warm."
msgstr "보온을 위해 %s을(를) 사용했다."

#: src/player_hardcoded_effects.cpp
msgctxt "memorial_male"
msgid "Entered hibernation."
msgstr "겨울잠에 접어든다."

#: src/player_hardcoded_effects.cpp
msgctxt "memorial_female"
msgid "Entered hibernation."
msgstr "겨울잠에 접어든다."

#: src/player_hardcoded_effects.cpp
