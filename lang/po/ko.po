msgstr "재래식 가상 화기와 탄약을 제거합니다."

#: lang/json/MOD_INFO_from_json.py
msgid "No Survivor Armor"
msgstr "생존용 방어구 제거"

#. ~ Description for No Survivor Armor
#: lang/json/MOD_INFO_from_json.py
msgid "Removes survivor armor."
msgstr "생존용 방어구를 제거합니다."

#: lang/json/MOD_INFO_from_json.py
msgid "No Triffids"
msgstr "트리피드 제거"

#. ~ Description for No Triffids
#: lang/json/MOD_INFO_from_json.py
msgid "Removes triffids from the game."
msgstr "트리피드를 제거합니다."

#: lang/json/MOD_INFO_from_json.py
