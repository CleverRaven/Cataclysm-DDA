msgstr "월간 새장(book)"

#: lang/json/BOOK_from_json.py
msgid "Bon Appetit"
msgstr "좋은 식욕(book)"

#: lang/json/BOOK_from_json.py
msgid "Boxing Monthly"
msgstr "월간 복싱(book)"

#: lang/json/BOOK_from_json.py
msgid "Building for Beginners"
msgstr "초보자들을 위한 건축방법(book)"

#: lang/json/BOOK_from_json.py
msgid "CQB Monthly"
msgstr "월간 백병전(book)"

#: lang/json/BOOK_from_json.py
msgid "Close Quarter Fighting Manual"
msgstr "백병전 메뉴얼(book)"

#: lang/json/BOOK_from_json.py
msgid "Computer Gaming"
msgstr "컴퓨터 게임(book)"

#: lang/json/BOOK_from_json.py
msgid "Computer Science 101"
msgstr "컴퓨터 과학 101(book)"

#: lang/json/BOOK_from_json.py
msgid "Computer Science 301"
msgstr "컴퓨터 과학 301(book)"

#: lang/json/BOOK_from_json.py
msgid "Computer World"
msgstr "컴퓨터 세상(book)"

