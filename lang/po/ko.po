msgstr "사격술"

#: lang/json/skill_from_json.py
msgid "mechanics"
msgstr "기계 공학"

#: lang/json/skill_from_json.py
msgid "melee"
msgstr "근접전투"

#: lang/json/skill_from_json.py
msgid "piercing weapons"
msgstr "관통 무기"

#: lang/json/skill_from_json.py
msgid "rifles"
msgstr "소총"

#: lang/json/skill_from_json.py
msgid "shotguns"
msgstr "산탄총"

#: lang/json/skill_from_json.py
msgid "speaking"
msgstr "대화술"

#: lang/json/skill_from_json.py
msgid "submachine guns"
msgstr "기관단총"

