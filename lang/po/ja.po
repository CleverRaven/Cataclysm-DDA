msgid "<npcname> thrashes %s"
msgstr "<npcname>の激打が%sに命中しました。"

#: src/melee.cpp
#, c-format
msgid "You thrash %s"
msgstr "激打が%sに命中しました。"

#: src/melee.cpp
#, c-format
msgid "<npcname> batters %s"
msgstr "<npcname>の乱打が%sに命中しました。"

#: src/melee.cpp
#, c-format
msgid "You batter %s"
msgstr "乱打が%sに命中しました。"

#: src/melee.cpp
#, c-format
msgid "<npcname> whacks %s"
msgstr "<npcname>の殴打が%sに命中しました。"

#: src/melee.cpp
#, c-format
