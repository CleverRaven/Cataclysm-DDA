msgstr "機械整備"

#: lang/json/skill_from_json.py
msgid "melee"
msgstr "近接戦闘"

#: lang/json/skill_from_json.py
msgid "piercing weapons"
msgstr "刺突武器"

#: lang/json/skill_from_json.py
msgid "rifles"
msgstr "ライフル"

#: lang/json/skill_from_json.py
msgid "shotguns"
msgstr "ショットガン"

#: lang/json/skill_from_json.py
msgid "speaking"
msgstr "話術"

#: lang/json/skill_from_json.py
msgid "submachine guns"
msgstr "サブマシンガン"

#: lang/json/skill_from_json.py
