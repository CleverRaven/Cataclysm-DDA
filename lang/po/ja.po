#: lang/json/scenario_from_json.py
msgid "Abandoned Cabin"
msgstr "放棄された小屋"

#: lang/json/scenario_from_json.py
msgid ""
"Against all odds, you managed to make it back to the station house in one "
"piece.  Now the reanimated husks of your former comrades want to make sure "
"you never leave it again."
msgstr ""

#: lang/json/scenario_from_json.py
msgid "Ambush"
msgstr "迎撃"

#: lang/json/scenario_from_json.py
msgid "Anywhere"
msgstr ""

#: lang/json/scenario_from_json.py
msgid "Assault on Precinct Z"
msgstr "襲撃されたZ区域"

#: lang/json/scenario_from_json.py
msgid "Burning Building"
msgstr "燃え盛る建物"

#: lang/json/scenario_from_json.py
msgid "Challenge-Abandoned"
msgstr "挑戦放棄"

#: lang/json/scenario_from_json.py
msgid "Challenge-Lab"
msgstr "挑戦者を待つ研究所"

#: lang/json/scenario_from_json.py
msgid "Challenge-Really Bad Day"
msgstr "最悪の日"

#: lang/json/scenario_from_json.py
msgid "Enclosed Shelter"
msgstr "包囲されたシェルター"

#: lang/json/scenario_from_json.py
msgid "Evacuee"
msgstr "避難者"

#: lang/json/scenario_from_json.py
msgid "Experiment"
msgstr "実験"

#: lang/json/scenario_from_json.py lang/json/start_location_from_json.py
msgid "Fire Station"
msgstr "消防署"

#: lang/json/scenario_from_json.py
msgid "Forest"
msgstr "森"

