msgstr "langärmeliges Hemd"

#: lang/json/ARMOR_from_json.py
msgid "A long-sleeved cotton shirt."
msgstr "Ein langärmeliges Hemd."

#: lang/json/ARMOR_from_json.py
msgid "flag shirt"
msgstr "Flaggenshirt"

#: lang/json/ARMOR_from_json.py
msgid "A t-shirt embroidered with the pattern of an American flag."
msgstr "Ein T-Shirt, das mit dem Muster der Flagge der USA bestickt wurde."

#: lang/json/ARMOR_from_json.py
msgid "polo shirt"
msgstr "Polohemd"

#: lang/json/ARMOR_from_json.py
msgid "A short-sleeved cotton shirt, slightly thicker than a t-shirt."
msgstr "Ein kurzärmeliges Stoffhemd, etwas dicker als ein T-Shirt."

#: lang/json/ARMOR_from_json.py
msgid "dress shirt"
msgstr "Anzugshemd"

#: lang/json/ARMOR_from_json.py
msgid "A white button-down shirt with long sleeves. Looks professional!"
msgstr ""
"Ein weißes Button-Down-Hemd mit langen Ärmeln. Sieht professionell aus!"

#: lang/json/ARMOR_from_json.py
msgid "sheriff's shirt"
msgstr "Sherriffshemd"

#: lang/json/ARMOR_from_json.py
msgid "A tan button-down shirt with long sleeves."
msgstr "Ein beiges Button-Down-Shirt mit langen Ärmeln."

#: lang/json/ARMOR_from_json.py
msgid "camo tank top"
msgstr "Tarn-Muskelshirt"

#: lang/json/ARMOR_from_json.py
msgid "A sleeveless cotton shirt with camouflage dye. Very easy to move in."
msgstr ""
"Ein ärmelloses Stoffhemd mit Tarnfarbe. Es ist sehr leicht, "
"hereinzuschlüpfen."

#: lang/json/ARMOR_from_json.py
msgid "tank top"
msgstr "Muskelshirt"

#: lang/json/ARMOR_from_json.py
msgid "A sleeveless cotton shirt. Very easy to move in."
msgstr "Ein ärmelloses Stoffhemd. Es ist sehr leicht, hereinzuschlüpfen."

#: lang/json/ARMOR_from_json.py
msgid "bikini top"
msgstr "Bikinioberteil"

#: lang/json/ARMOR_from_json.py
msgid "A simple bikini top."
msgstr "Ein einfaches Bikinioberteil."

#: lang/json/ARMOR_from_json.py
msgid "leather bikini top"
msgstr "Lederbikinioberteil"

#: lang/json/ARMOR_from_json.py
msgid "A simple leather bikini top."
msgstr "Ein einfaches Lederbikinioberteil."

#: lang/json/ARMOR_from_json.py
msgid "fur bikini top"
msgstr "Fellbikinioberteil"

#: lang/json/ARMOR_from_json.py
