msgstr ""

#: lang/json/material_from_json.py
msgid "Wheat"
msgstr "пшеница"

#: lang/json/material_from_json.py
msgid "Junk Food"
msgstr "вредная пища"

#: lang/json/material_from_json.py
msgid "smashed"
msgstr "разбитый"

#: lang/json/material_from_json.py
msgid "Flesh"
msgstr "плоть"

#: lang/json/material_from_json.py
msgid "sliced"
