msgid "AI tag used for turret targeting sounds. This is a bug if you have it."
msgstr ""

#: lang/json/effects_from_json.py
msgid "Adrenaline Comedown"
msgstr "Спад адреналина"

#: lang/json/effects_from_json.py
msgid "Adrenaline Rush"
msgstr "Выброс адреналина"

#: lang/json/effects_from_json.py
msgid "Asked Info"
