msgstr ""

#: lang/json/json_parrot.py:227
msgid "a very loud beeping sound."
msgstr ""

#: lang/json/json_parrot.py:228
msgid "a loud hiss."
msgstr ""

#: lang/json/json_parrot.py:229
msgid "a loud crackling noise."
msgstr ""

#: lang/json/json_parrot.py:230
msgid "gunfire!"
msgstr ""

#: lang/json/json_parrot.py:231
