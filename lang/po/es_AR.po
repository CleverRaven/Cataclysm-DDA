msgid "Birdhouse Monthly"
msgid_plural "Birdhouse Monthlies"
msgstr[0] "Pajareras Mensual"
msgstr[1] "Pajareras Mensual"

#: lang/json/BOOK_from_json.py
msgid "Bon Appetit"
msgid_plural "Bon Appetits"
msgstr[0] "Bon Appetit"
